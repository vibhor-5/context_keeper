{
  "name": "contextkeeper-mcp-slack-bot",
  "version": "1.0.0",
  "description": "AI-powered repository context and team onboarding through MCP server and Slack bot integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:pbt": "vitest --run --reporter=verbose",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mcp",
    "slack",
    "ai",
    "repository",
    "context",
    "onboarding"
  ],
  "author": "ContextKeeper Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@slack/bolt": "^3.17.1",
    "@slack/web-api": "^7.0.2",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.10.4",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "fast-check": "^3.23.2",
    "jsdom": "^28.0.0",
    "supertest": "^7.2.2",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
